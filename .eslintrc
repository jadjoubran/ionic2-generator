{
  "rules": {
    "indent": [
      0,
      4, {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      2,
      "never"
    ],
    "default-case": 1,
    "no-unused-vars": [2, {
      "vars": "all",
      "args": "after-used"
    }],
    "eqeqeq": 1,
    "no-eq-null": 1,
    "no-alert": 1,
    "no-eval": 2,
    "no-loop-func": 1,
    "no-param-reassign": 1,
    "wrap-iife": [2, "inside"],
    "init-declarations": 1,
    "no-undefined": 2,
    "no-lonely-if": 1,
    "no-trailing-spaces": 1,
    "no-unneeded-ternary": 1,
    "no-bitwise": 1,
    "yoda": 2,
    "no-mixed-spaces-and-tabs": [2],
    "no-console": 0
  },
  "globals": {
    "require": true
  },
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "extends": "eslint:recommended",
}
